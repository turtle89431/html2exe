    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "asar": "0.14.x",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cmake-binaries": "1.0.5-2",
    "download-yue": "2.x",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-clean-easy": "^1.0.2",
    "yazl": "2.4.3"
  }
